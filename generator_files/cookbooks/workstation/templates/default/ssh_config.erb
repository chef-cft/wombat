Host <%= @server %>
  HostName <%= @server %>
  Port 8989
  User <%= @automate_user -%>@<%= @ent %>
  IdentityFile <%= @home -%>/.ssh/id_rsa
  IdentitiesOnly yes
  KexAlgorithms +diffie-hellman-group1-sha1
  StrictHostKeyChecking no

Host *
  Port 22
  User <%= @ssh_user %>
  IdentityFile <%= @home -%>/.ssh/id_rsa
  IdentitiesOnly yes
  KexAlgorithms +diffie-hellman-group1-sha1
  StrictHostKeyChecking no
